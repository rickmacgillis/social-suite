{
  "name": "social-suite",
  "version": "1.0.0",
  "description": "Open source social post scheduling system",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js",
    "watch": "env-cmd -f config/dev.env nodemon src/index.js",
    "test": "env-cmd -f ./config/test.env jest --watch --runInBand"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "jest-extended"
    ]
  },
  "keywords": [
    "social",
    "post",
    "hootsuite",
    "schedule",
    "facebook",
    "twitter",
    "linkedin",
    "instagram"
  ],
  "author": "Rick Mac Gillis",
  "license": "MIT",
  "dependencies": {
    "@sendgrid/mail": "^6.5.0",
    "bcrypt": "^3.0.7",
    "cron": "^1.8.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "mongodb": "^3.5.1",
    "mongoose": "^5.8.9",
    "node-uuid": "^1.4.8",
    "passport": "^0.4.1",
    "passport-linkedin-oauth2": "^2.0.0",
    "password-validator": "^5.0.3",
    "social-suite-angular": "git+ssh://git@github.com:rickmacgillis/social-suite-angular.git",
    "twitter": "^1.7.1",
    "validator": "^12.1.0"
  },
  "devDependencies": {
    "env-cmd": "^10.0.1",
    "jest": "^24.9.0",
    "jest-extended": "^0.11.2",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2"
  }
}
